---
layout: default
pagination:
  data: allSubjects
  size: 1
  alias: subject
permalink: "/groups/{{ subject.group.name }}/subjects/{{ subject.zooniverse_id}}/"
---
<div class="subject">
  <div>
    {% SVGSubject subject=subject %}
      {% for mark in results.reduced %}
        {% SVGMark mark=mark %}
      {% endfor %}
    {% endSVGSubject %}
  </div>
  <div>
    <h2>Keywords</h2>
    <p>
      {{ results.keywords | join(', ') }}
    </p>

    <h2>Metadata</h2>
    {% for key, value in subject.metadata %}
      <dt>{{ key }}</dt>
      <dd>{{ value }}</dd>
    {% endfor %}
  </div>
</div>

<h2>Drawn marks</h2>
<dl>
{% for reduction in results.reduced %}
  <dt>{{ reduction.type }}</dt>
  <dd>{{ reduction.value | dump }}</dd>
{% endfor %}
</dl>

<h2>Raw result</h2>
<dl>
{% for key, result in results %}
  <dt>{{ key }}</dt>
  <dd>{{ result | dump }}</dd>
{% endfor %}
</dl>
