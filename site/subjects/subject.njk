---
layout: default
pagination:
  data: allSubjects
  size: 1
  alias: subject
permalink: "/groups/{{ subject.group.name }}/subjects/{{ subject.zooniverse_id}}/"
---
<div class="subject">
  <div>
    {% SVGSubject subject=subject %}
      {% for mark in result.reduced %}
        {% SVGMark mark=mark %}
      {% endfor %}
    {% endSVGSubject %}
  </div>
  <div>
    <h2>Keywords</h2>
    <p>
    {% for keyword in linkedKeywords %}
      {% if keyword.href %}
        <a href="{{ keyword.href }}">{{ keyword.key }}</a> 
      {% else %}
        {{ keyword.key }} 
      {% endif %}
    {% endfor %}
    </p>

    <h2>Contributors</h2>
    <ul>
    {% for contributor in contributors %}
      <li><a href="{{ contributor.href }}">{{ contributor.name }}</a></li>
    {% endfor %}
    </ul>

    <h2>Metadata</h2>
    <dl>
    {% for key, value in subject.metadata %}
      <dt>{{ key }}</dt>
      <dd>{{ value | dump }}</dd>
    {% endfor %}
    </dl>
  </div>
</div>

<h2>Drawn marks</h2>
<dl>
{% for reduction in result.reduced %}
  <dt>{{ reduction.type }}</dt>
  <dd>{{ reduction.value | dump }}</dd>
{% endfor %}
</dl>

<h2>Raw result</h2>
<dl>
{% for key, value in result %}
  <dt>{{ key }}</dt>
  <dd>{{ value | dump }}</dd>
{% endfor %}
</dl>
